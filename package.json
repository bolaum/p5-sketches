{
  "name": "p5.js-es6",
  "version": "1.0.0",
  "description": "Boilerplate for writing p5.js sketches in es6 with babel, webpack and sass.",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --progress --colors --inline --hot --open",
    "build": "npm run clean && webpack --progress --colors -p",
    "buildProd": "npm run build -- --mode production --devtool false --output-public-path '/p5-sketches/'",
    "deploy": "gh-pages-deploy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mkontogiannis/p5.js-es6.git"
  },
  "author": "Michael Kontogiannis <mike@kontogiannis.io> (http://m.kontogiannis.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkontogiannis/p5.js-es6/issues"
  },
  "homepage": "https://github.com/mkontogiannis/p5.js-es6#README",
  "browserslist": [
    "defaults",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "foundation-sites": "^6.5.3",
    "jquery": "^3.4.1",
    "lodash": "^4.17.14",
    "navigo": "^7.1.2",
    "p5": "^0.9.0",
    "showdown": "^1.9.0",
    "what-input": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "ejs-loader": "^0.3.3",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "gh-pages-deploy": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "module-to-cdn": "^3.1.5",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier-eslint": "^9.0.0",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "dynamic-import-webpack",
      "@babel/transform-runtime"
    ]
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "buildProd"
    ],
    "post": [
      "clean"
    ],
    "noprompt": false
  }
}
